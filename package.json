{
  "name": "our-wedding-heroes",
  "version": "1.0.0",
  "description": "Wedding website with gift list",
  "private": true,
  "scripts": {
    "clean": "rimraf ./build",
    "prebuild": "yarn run clean",
    "build": "NODE_ENV=production webpack --config ./config/webpack.config.prod.js",
    "heroku-postbuild": "yarn run build",
    "lint-css": "stylelint \"./public/**/*.{js,jsx}\"",
    "lint-stylus": "stylint ./public",
    "lint-js": "eslint --ignore-path .gitignore --ignore-pattern flow-typed/ --ext .js,.jsx .",
    "lint": "yarn run lint-css && yarn run lint-stylus && yarn run lint-js",
    "test": "yarn run lint && yarn run flow check && jest --coverage",
    "test:watch": "jest --watch",
    "start": "node ./server/index.js",
    "start:dev": "nodemon --ignore public ./server/index.js",
    "format": "prettier --print-width 160 --trailing-comma es5 --single-quote --write './*.js' './!(flow-typed|node_modules|coverage)/**/*.{js,jsx}'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --print-width 160 --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "repository": {
    "url": "git+https://insidersbyte@github.com/InsidersByte/our-wedding-heroes.git",
    "type": "git"
  },
  "author": "Jonathon Kelly <jonathon@insidersbyte.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/InsidersByte/our-wedding-heroes/issues"
  },
  "homepage": "https://github.com/InsidersByte/our-wedding-heroes#readme",
  "dependencies": {
    "@insidersbyte/react-markdown-editor": "0.3.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.3.2",
    "body-parser": "1.17.1",
    "bookshelf": "0.10.3",
    "bookshelf-camelcase": "1.1.4",
    "co": "4.6.0",
    "cors": "2.8.3",
    "dotenv": "4.0.0",
    "email-templates": "2.5.4",
    "express": "4.15.2",
    "express-jwt": "5.1.0",
    "express-validator": "3.1.3",
    "jsonwebtoken": "7.3.0",
    "jwt-decode": "2.2.0",
    "knex": "0.12.9",
    "material-ui": "0.17.1",
    "moment": "2.18.1",
    "nodemailer": "2.7.2",
    "object.values": "1.0.4",
    "pg": "6.1.5",
    "pug": "2.0.0-beta11",
    "react": "15.4.2",
    "react-dnd": "2.3.0",
    "react-dnd-html5-backend": "2.3.0",
    "react-dom": "15.4.2",
    "react-loader": "2.4.0",
    "react-markdown-renderer": "1.2.0",
    "react-notification-system": "0.2.13",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-actions": "2.0.1",
    "redux-thunk": "2.2.0",
    "smoothscroll": "0.3.0",
    "styled-components": "1.4.4",
    "superagent": "3.5.2",
    "uuid": "3.0.1",
    "validator": "7.0.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-loader": "6.4.1",
    "babel-plugin-flow-react-proptypes": "0.21.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-react": "6.23.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.22.0",
    "case-sensitive-paths-webpack-plugin": "2.0.0",
    "coveralls": "2.13.0",
    "css-loader": "0.28.0",
    "empty": "0.10.1",
    "enzyme": "2.8.0",
    "enzyme-to-json": "1.5.0",
    "errorhandler": "1.5.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-config-prettier": "1.5.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.30.4",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "flow-bin": "0.43.0",
    "flow-typed": "2.0.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "husky": "0.13.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "19.0.2",
    "lint-staged": "3.4.0",
    "morgan": "1.8.1",
    "nodemon": "1.11.0",
    "postcss-loader": "1.3.3",
    "prettier": "0.22.0",
    "react-addons-test-utils": "15.4.2",
    "react-dev-utils": "0.5.2",
    "redux-devtools": "3.3.2",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.2.0",
    "redux-immutable-state-invariant": "2.0.0",
    "redux-logger": "3.0.1",
    "rimraf": "2.6.1",
    "style-loader": "0.16.1",
    "stylelint": "7.10.1",
    "stylelint-config-standard": "16.0.0",
    "stylelint-processor-styled-components": "0.1.0",
    "stylint": "1.5.9",
    "stylint-stylish": "1.4.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.18.0",
    "webpack-manifest-plugin": "1.1.0"
  },
  "engines": {
    "node": "6.10.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "public/**/*.{js,jsx}",
      "server/**/*.js",
      "lib/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|styl)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "./config/setupTests"
  }
}
