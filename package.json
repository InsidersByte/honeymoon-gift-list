  "description": "Wedding website with gift list",
  "main": "./server",
  "scripts": {
    "clean": "rimraf dist",
    "postinstall": "npm run build",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack --config webpack.production.config.js --profile --colors",
    "build:progress": "npm run build -- --progress",
    "lint:stylus": "stylint ./public",
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "lint:js-fix": "npm run lint:js -- --fix",
    "lint": "npm run lint:stylus && npm run lint:js",
    "test": "npm run lint",
    "start": "node ./server/index.js",
    "start:dev": "nodemon --ignore public ./server/index.js"
  },
